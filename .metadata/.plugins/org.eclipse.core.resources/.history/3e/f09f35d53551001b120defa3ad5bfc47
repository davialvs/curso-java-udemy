package _02testeprocessamento;

public class Divisao {

	public static void main(String[] args) {
		
		int a = 5;
		int b = 2;
		double resultado;
		
		resultado = a / b;
		
		// Resultado será em números inteiros pois o compilador corta as casas decimais.
		System.out.println(resultado);
		
		//Para não truncar as casas decimais:
		resultado = (double) a / b;
		System.out.println(resultado);

	}

}
