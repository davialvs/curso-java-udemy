package funcoesmatematicas;

import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
		
		 Scanner sc = new Scanner(System.in);
		 
		 double raio;
		 double area;
		 double pot;
		 
		 raio = sc.nextDouble();
		 
		 pot = Math.pow(raio, 2.0);
		 
		 area = 3.14159 * pot;
		 
		 System.out.printf("A = %f.4%n", area);
		 
	}

}
